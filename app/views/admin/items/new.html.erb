<h1>商品新規登録</h1>
<!-- form_with部分 -->
<%= form_with model: Item.new do |f| %>
 <!-- 商品画像と税抜き価格のリンクまだ-->
  <h4>商品画像</h4>
   <%= f.file_field :image, accept: "image/*" %>
   
  <h4>商品名</h4>
  <%= f.text_field :title %>
	
  <h4>商品説明</h4>
  <%= f.text_area :body %>
  
  <h4>税抜価格</h4>
	 <%= f.text_field :price %><span>円</span>

  <%= f.submit '新規登録' %>
<% end %>

